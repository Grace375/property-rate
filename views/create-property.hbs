<div id="page-wrapper">
    <div class="main-page">
        <div class="forms">
            <div class="row">
                <div class="form-three widget-shadow">
                    <div class=" panel-body-inputin">
                        <form class="form-horizontal">
                            <div class="form-group">
                                <label class="col-md-2 control-label">Owner</label>
                                <div class="col-md-8">
                                    <div class="input-group">
												<span class="input-group-addon">
													<i class="fa fa-envelope-o"></i>
												</span>
                                        <input type="text" class="form-control1" placeholder="owner" name="owner">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-2 control-label">Property ID</label>
                                <div class="col-md-8">
                                    <div class="input-group">
												<span class="input-group-addon">
													<i class="fa fa-key"></i>
												</span>
                                        <input type="text" class="form-control1" id="exampleInputPassword1"
                                               placeholder="Property_id" name="num">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-2 control-label">Location</label>
                                <div class="col-md-8">
                                    <div class="input-group input-icon right">
												<span class="input-group-addon">
													<i class="fa fa-envelope-o"></i>
												</span>
                                        <input class="form-control1" type="text"
                                               placeholder="Location" name="location">
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <p class="help-block">With tooltip</p>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-2 control-label">Area</label>
                                <div class="col-md-8">
                                    <div class="input-group input-icon right">
												<span class="input-group-addon">
													<i class="fa fa-envelope-o"></i>
												</span>
                                        <input type="text" class="form-control1" placeholder="Area" name="area">
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <p class="help-block">With tooltip</p>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-2 control-label">Use code</label>
                                <div class="col-md-8">
                                    <div class="input-group input-icon right">
												<span class="input-group-addon">
													<i class="fa fa-envelope-o"></i>
												</span>
                                        <select class="form-control1"  name="use_code" id="use_code">
                                            {{#each use_code}}
                                            <option label="{{code}}--{{name}}--{{rate}}" value="{{code}}"></option>
                                            {{/each}}

                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <p class="help-block">Use_code is valid!</p>
                                </div>
                            </div>

                            <div class="form-group has-error">
                                <label class="col-md-2 control-label">Rate_able Value</label>
                                <div class="col-md-8">
                                    <div class="input-group input-icon right">
												<span class="input-group-addon">
													<i class="fa fa-envelope-o"></i>
												</span>
                                        <input type="number" class="form-control1" placeholder="Rate_able value" name="rate_val">
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <p class="help-block">Error!</p>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-2 control-label">Sanitation code</label>
                                <div class="col-md-8">
                                    <div class="input-icon right spinner">
                                        <i class="fa fa-fw fa-spin fa-spinner"></i>
                                        <select class="form-control1"
                                                name="sanitation_code">
                                            {{#each sanitation}}
                                                <option label="{{code}}: {{name}}" value="{{code}}"></option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-2">
                                    <p class="help-block">Processing right</p>
                                </div>
                            </div>


                        </form>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
</div>